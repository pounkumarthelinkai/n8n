# N8N DEV Environment Configuration
# Copy this file to /srv/n8n/.env on your DEV VPS

# N8N Settings
N8N_ENCRYPTION_KEY=CHANGE_ME_GENERATE_WITH_openssl_rand_base64_32
N8N_HOST=dev-n8n.yourdomain.com
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://dev-n8n.yourdomain.com
N8N_EDITOR_BASE_URL=http://dev-n8n.yourdomain.com
GENERIC_TIMEZONE=UTC
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console

# Database Settings
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=CHANGE_ME_SECURE_PASSWORD

# Postgres Settings
POSTGRES_USER=n8n
POSTGRES_PASSWORD=CHANGE_ME_SECURE_PASSWORD
POSTGRES_DB=n8n
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=CHANGE_ME_SECURE_PASSWORD

# Environment
N8N_ENV=dev

