# N8N PRODUCTION Environment Configuration
# Copy this file to /srv/n8n/.env on your PROD VPS

# IMPORTANT: Use DIFFERENT encryption key than DEV!

# N8N Settings
N8N_ENCRYPTION_KEY=CHANGE_ME_DIFFERENT_FROM_DEV_openssl_rand_base64_32
N8N_HOST=n8n.yourdomain.com
N8N_PORT=5678
N8N_PROTOCOL=https
WEBHOOK_URL=https://n8n.yourdomain.com
N8N_EDITOR_BASE_URL=https://n8n.yourdomain.com
GENERIC_TIMEZONE=UTC
N8N_LOG_LEVEL=warn
N8N_LOG_OUTPUT=file

# Database Settings
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=CHANGE_ME_STRONG_PRODUCTION_PASSWORD

# Postgres Settings
POSTGRES_USER=n8n
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PRODUCTION_PASSWORD
POSTGRES_DB=n8n
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=CHANGE_ME_STRONG_PRODUCTION_PASSWORD

# Environment
N8N_ENV=production

# Security Settings
N8N_SECURE_COOKIE=true
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true

